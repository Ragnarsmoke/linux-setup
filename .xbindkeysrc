# Increase volume
"pamixer --increase 5"
   XF86AudioRaiseVolume

# Decrease volume
"pamixer --decrease 5"
   XF86AudioLowerVolume

"xfce4-screenshooter"
    Mod2+Print

# Terminal
"urxvt"
    control+alt+t

# Keystroke launcher
"rofi -combi-modi window,drun -show drun"
    control+alt+r

# Firefox
"firefox"
    control+alt+w

# Thunderbird
"thunderbird"
    control+alt+m
