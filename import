#!/usr/bin/bash

# Tmux
cp ~/.tmux.conf .
cp ~/.tmux-theme.conf .

# Vim
cp ~/.vimrc .

# Keybinds
cp ~/.xbindkeysrc .

# UXvrt Terminal
cp ~/.Xdefaults .

# ZSH
cp ~/.zshrc .

# Rofi
cp /usr/local/bin/rofi-custom .

# Openbox
cp ~/.config/openbox/rc.xml ./openbox
cp ~/.config/openbox/autostart ./openbox

# Tint 2
cp /usr/share/tint2/numix-customized-emil.tint2rc ./tint2rc

# Wallpaper
cp /usr/share/backgrounds/wallpaper-emil.jpg ./wallpaper.jpg
cp ~/.config/nitrogen/bg-saved.cfg .
