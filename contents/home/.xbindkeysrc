# Screenshots
"xfce4-screenshooter"
    Mod2+Print

# Terminal
"urxvt"
    control+alt+t

# Ranger
"urxvt -e ranger"
    control+alt+e

# Keystroke launcher
"rofi -combi-modi window,drun -show drun"
    control+alt+r

# Firefox
"qutebrowser"
    control+alt+w

# Thunderbird
"thunderbird"
    control+alt+m

# Media control
"pamixer --increase 5"
    XF86AudioRaiseVolume

"pamixer --decrease 5"
    XF86AudioLowerVolume

"pamixer -t"
    XF86AudioMute

"playerctl-toggle-play"
    XF86AudioPlay

"playerctl next"
    XF86AudioNext

"playerctl previous"
    XF86AudioPrev

# Screen control
"xbacklight -inc 20"
    XF86MonBrightnessUp

"xbacklight -dec 20"
    XF86MonBrightnessDown
