# Volume control
"pamixer --increase 5"
   XF86AudioRaiseVolume

"pamixer --decrease 5"
   XF86AudioLowerVolume

"pamixer -t"
    Mod2+Mod4+t

# Screenshots
"xfce4-screenshooter"
    Mod2+Print

# Terminal
"urxvt"
    control+alt+t

# Ranger
"urxvt -e ranger"
    control+alt+e

# Keystroke launcher
"rofi -combi-modi window,drun -show drun"
    control+alt+r

# Firefox
"qutebrowser"
    control+alt+w

# Thunderbird
"thunderbird"
    control+alt+m
